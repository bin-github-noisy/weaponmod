{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors_list}"
  ],
  "contact": {
    "homepage": "${mod_home_url}",
    "sources": "${mod_sources_url}",
    "issues": "${mod_issues_url}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "bin.flintlock.fabric.BalkonsWeaponModFabric"
    ],
    "modmenu": [
      "bin.flintlock.fabric.BalkonsWeaponModModMenuIntegration"
    ]
  },
  "mixins": [
    "flintlock.mixins.json"
  ],
  "accessWidener": "flintlock.accesswidener",
  "depends": {
    "fabric": "*",
    "minecraft": "${minecraft_version_condition}",
    "architectury": ">=${architectury_version}",
    "cloth-config2": ">=${cloth_config_version}"
  }
}
